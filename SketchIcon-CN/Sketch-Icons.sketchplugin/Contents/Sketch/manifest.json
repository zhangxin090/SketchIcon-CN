{
  "compatibleVersion": 47,
  "bundleVersion": 1,
  "version": "4.0.5",
  "name": "Sketch图标",
  "identifier": "com.amoreaux.sketch-icons",
  "description": "Import your icons, apply a color mask and create a dynamic icons library.",
  "authorEmail": "moreaux.antoine@gmail.com",
  "author": "Valentin Chrétien, Antoine Moreaux",
  "appcast": "https://raw.githubusercontent.com/AMoreaux/Sketch-Icons/master/.appcast.xml",
  "icon": "icons/icon-square-1.jpg",
  "commands": [
    {
      "script": "commands.js",
      "handler": "importIcons",
      "shortcut": "command shift i",
      "name": "导入图标...",
      "identifier": "import.icons",
      "description": "Import your icons library",
      "icon": "icons/icon-square-1.jpg"
    },
    {
      "script": "commands.js",
      "handler": "updateIconsOnSelectedArtboards",
      "name": "替换图标...",
      "identifier": "update.icons.selected.artboard",
      "icon": "icons/icon-square-1.jpg",
      "description": "Replace icons on selected artboards"
    },
    {
      "script": "commands.js",
      "handler": "addMaskOnSelectedArtboards",
      "shortcut": "command shift m",
      "name": "应用色彩蒙版...",
      "identifier": "mask.selected.artboard",
      "icon": "icons/icon-square-1.jpg",
      "description": "Apply mask on selected artboards"
    },
    {
      "script": "commands.js",
      "handler": "addMaskOnSelectedArtboards",
      "name": "替换色彩蒙版...",
      "identifier": "update.mask.selected.artboard",
      "icon": "icons/icon-square-1.jpg",
      "description": "Replace mask on selected artboard"
    },
    {
      "script": "commands.js",
      "handler": "removeMaskOnSelectedArtboards",
      "name": "移除色彩蒙版",
      "identifier": "remove.mask.selected.artboard",
      "icon": "icons/icon-square-1.jpg",
      "description": "Remove mask on selected artboard"
    },
    {
      "script": "commands.js",
      "handler": "setSettings",
      "name": "设置",
      "identifier": "settings.sketch.icons",
      "shortcut": "command shift f",
      "icon": "icons/icon-square-1.jpg",
      "description": "Configure sketch icons"
    },
    {
      "script": "commands.js",
      "handler": "organizeIcons",
      "name": "组织图标...",
      "identifier": "organize.icons.selected.layer",
      "icon": "icons/icon-square-1.jpg",
      "description": "Organize selected icons"
    }
  ],
  "menu": {
    "title": "Sketch图标",
    "items": [
      "import.icons",
      "update.icons.selected.artboard",
      "organize.icons.selected.layer",
      "-",
      "mask.selected.artboard",
      "update.mask.selected.artboard",
      "remove.mask.selected.artboard",
      "-",
      "settings.sketch.icons"
    ]
  },
  "disableCocoaScriptPreprocessor": true
}
